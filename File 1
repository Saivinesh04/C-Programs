Auto:
#include<stdio.h>
int printvalue()
{
    auto int a=10;
    printf("%d",a);
}
int main(){
    printvalue();
    return 0;
    
}
Break and continue:
#include<stdio.h>
int main(){
    for(int i=1;i<=10;i++)
    {
        if(i==3)
        {
            continue;
        }
        if(i==7)
        {
            break;
        }
        printf("%d",i);
    }
    return 0;
}
Switch, case and default:
#include<stdio.h>
int main(){
    int i=3;
    switch(i){
        case 1:
        printf("case 1\n");break;
        case 2:
        printf("case 2\n");break;
        case 3:
        printf("case 3\n");break;
        case 4:
        printf("case 4\n");break;
        default:
        printf("Default\n");break;
    }
    return 0;
}
